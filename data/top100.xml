              <item>Butler (extracts from 'Hudibras' and 'Remains').</item> <!-- 1 item (2 vol.) containing specified works in SBL. -->
              <item>Collins.</item> <!-- 2 authors; W. (1 item, poetry) and John C. (1 item, history) in SBL. -->
              <item>Gray.</item> <!-- 1 item in SBL. -->
              <item>Herrick.</item> <!-- 3 items in SBL. -->
              <item>Suckling.</item> <!-- 1 item in SBL. -->
              <item>Prior.</item> <!-- Not found in SBL. -->
              <item>Etherege.</item> <!-- 1 item in SBL. -->
              <item>Drayton.</item> <!-- 2 authors; John (1 item) and M. (1 item). -->
              <item>George Herbert.</item> <!-- 1 item in SBL. -->
              <item>Crashaw.</item> <!-- 1 item in SBL. -->
              <item>Randolph.</item> <!-- 1 item in SBL. -->
              <item>Wither.</item> <!-- Not found in SBL. -->
              <item>La Fontaine.</item> <!-- Not found in SBL. -->
              <item>Voltaire.</item> <!-- 8 items in SBL. -->
              <item>Diderot.</item> <!-- 1 item in SBL. -->
              <item>Chamfort (Maxims).</item> <!-- Not found in SBL. -->
              <item>Beaumarchais.</item> <!-- Not found in SBL. -->
              <item>Stendhal.</item> <!-- 1 item (translated) in SBL. -->
              <item>Dumas.</item> <!-- 5 items in SBL. -->
              <item>Jane Austen.</item> <!-- 2 items (volumes of novels) in SBL. -->
              <item>Charlotte Bronte.</item> <!-- 2 items (1 is all three sisters) in SBL. -->
